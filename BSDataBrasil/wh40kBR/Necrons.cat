    </profile>
    <profile hidden="false" id="2feb-ae85-c8e0-9f61" name="Phaeron of the Stars (Aura)" typeId="b289-e7b4-432c-e2dd" typeName="Triarch Abilities">
     <characteristics>
      <characteristic name="Effect" typeId="a55f-e008-7f78-1890">
