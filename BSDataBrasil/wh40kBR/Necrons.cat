    </profile>
    <profile hidden="false" id="a8bf-159-583c-5973" name="Bringer of Unity (Aura)" typeId="b289-e7b4-432c-e2dd" typeName="Triarch Abilities">
     <characteristics>
      <characteristic name="Effect" typeId="a55f-e008-7f78-1890">
