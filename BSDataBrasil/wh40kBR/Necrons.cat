      </characteristic>
     </characteristics>
    </profile>
    <profile hidden="false" id="a7b-b3f0-3fa3-85a" name="The Silent King" typeId="9cc3-6d83-4dd3-9b64" typeName="Abilities">
     <characteristics>
      <characteristic name="Description" typeId="9b8f-694b-e5e-b573">
